package main;

import com.notnoop.apns.*;

public class Notifications {

	public static void main(String[] args) {
		
		ApnsService service =
			    APNS.newService()
			    .withCert("/Users/Zimtstern/Desktop/PushNotification.p12", "Findus1993")
			    .withSandboxDestination()
			    .build();
		
		String payload = APNS.newPayload().alertBody("Hallo").build();
		String token = "duda";
		service.push(token, payload);

	}

}
